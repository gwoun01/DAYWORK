  "description": "이노맥스 업무관리 프로그램",
  "main": "server/mainServer.ts",
  "scripts": {
    "dev:server": "ts-node-dev --respawn --transpile-only --project tsconfig.server.json server/00_mainServer.ts",
    "dev:frontend": "webpack --watch --mode development",
    "dev:css": "postcss TypeScript/styles/styles.css -o docs/assets/css/styles.css --watch",
    "dev:all": "concurrently \"npm run dev:frontend\"",
    "build:frontend": "webpack --mode production",
    "build:server": "tsc -p tsconfig.server.json",
    "build:css": "postcss TypeScript/styles/styles.css -o docs/assets/css/styles.css",
    "build": "npm run build:frontend && npm run build:css"
  },
  "dependencies": {
    "@types/nodemailer": "^7.0.3",
