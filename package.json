    "chart.js": "^4.5.0",
    "coolsms-node-sdk": "^2.0.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.18",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "html5-qrcode": "^2.3.8",
    "nodemailer": "^7.0.10",
    "pg": "^8.16.3",
    "qrcode": "^1.5.3",
    "sheetjs-style": "^0.15.8",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5"
